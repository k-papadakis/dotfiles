              {
                "from": {
                  "key_code": "m",
                  "modifiers": {
                    "mandatory": ["control"],
                    "optional": ["any"]
                  }
                },
                "to": [
                  {
                    "key_code": "return_or_enter"
                  }
                ],
                "type": "basic",
                "conditions": [
                  {
                    "type": "frontmost_application_unless",
                    "bundle_identifiers": [
                      "^org.alacritty$",
                      "^com.googlecode.iterm2$"
                    ]
                  }
                ]
              }
            ]
          },
          {
            "description": "Control+h to Backspace if not in Terminal",
            "manipulators": [
              {
                "from": {
                  "key_code": "h",
                  "modifiers": {
                    "mandatory": ["control"],
                    "optional": ["any"]
                  }
                },
                "to": [
                  {
                    "key_code": "delete_or_backspace"
                  }
